
  // jumpInCols Plugin

(function($){

  $.fn.jumpInCols = function(value) {

    this.each(function() {
      var layer = $(this).getLayer();
      var currentPosition = layer.frame().x();
      var finalPosition = currentPosition + value;
      layer.frame().setX(finalPosition);
    });

      return this;

   };

}(sQuery));
